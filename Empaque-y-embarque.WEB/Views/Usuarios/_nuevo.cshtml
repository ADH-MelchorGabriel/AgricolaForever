
<div class="modal" id="modalNuevo" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Agricultor</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close" v-on:click="cerrarModalNuevo">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">

				<div class="row">
					<div class="form-group">
						<label for="nombre">UserName</label>
						<input v-model="obj.userName" type="text" placeholder="Ej. JReneMedina" class="form-control" style="width:100%" />
					</div>
					<div class="form-group">
						<label for="password">Password</label>
						<input v-model="obj.password" type="password" placeholder="Importante no olvidar tu password"  maxlength="8" class="form-control" style="width:100%" />
						
					</div>
				</div>

				<div class="row">
					<div class="form-group">
						<label for="nombre">Nombre</label>
						<input v-model="obj.nombre" type="text" placeholder="Ej. Jesus Rene Medina Sanchez" class="form-control" style="width:100%" />
					</div>
					<div class="form-group">
						<label for="correo">Correo</label>
						<input v-model="obj.correo" type="text" placeholder="CorreoRene@correo.com" class="form-control" style="width:100%" />
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary" v-on:click="guardar">Guardar</button>
				<button type="button" class="btn btn-secondary" data-dismiss="modal" v-on:click="cerrarModalNuevo">Cerrar</button>
			</div>
		</div>
	</div>
</div>
